{
  "name": "ai-core-client-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Lib,Themes\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:lib\" \"npm run watch.scss\" ",
    "watch:lib": "ng build @aitheon/core-client --watch",
    "build": "npm run build:lib && npm run build.scss && npm run copy:assets && npm run copy:variables",
    "copy:assets": "cpx \"projects/aitheon/core-client/src/assets/**/*\" dist/aitheon/core-client/assets",
    "copy:variables": "cpx \"projects/aitheon/core-client/src/styles/**/*variables.scss\" dist/aitheon/core-client/styles",
    "build.scss": "node-sass --recursive projects/aitheon/core-client/src/styles --output-style compact --output dist/aitheon/core-client/styles --source-map true --source-map-contents scss",
    "watch.scss": "node-sass --recursive projects/aitheon/core-client/src/styles --output dist-styles --source-map true --source-map-contents scss && node-sass -w --recursive projects/aitheon/core-client/src/styles --output-style compact --output dist-styles --source-map true --source-map-contents scss",
    "build:demo": "rimraf dist/demo && ng build",
    "build:lib": "ng build @aitheon/core-client",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pub:lib": "npm run build && npm publish dist/aitheon/core-client"
  },
  "private": true,
  "dependencies": {
    "@aitheon/ai-web-client-angular": "^0.0.5",
    "@aitheon/ckeditor": "^0.4.2",
    "@angular/animations": "~7.2.3",
    "@angular/cdk": "^7.2.3",
    "@angular/common": "~7.2.3",
    "@angular/compiler": "~7.2.3",
    "@angular/core": "~7.2.3",
    "@angular/forms": "~7.2.3",
    "@angular/http": "~7.2.3",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.3",
    "@angular/platform-browser-dynamic": "~7.2.3",
    "@angular/router": "~7.2.3",
    "@ckeditor/ckeditor5-angular": "1.1.2",
    "@ng-select/ng-select": "2.15.2",
    "@sweetalert2/ngx-sweetalert2": "5.0.1",
    "bootstrap": "^4.3.1",
    "concurrently": "4.1.0",
    "copyfiles": "2.1.0",
    "core-js": "^2.6.3",
    "cpx": "1.5.0",
    "fabric": "^3.5.1",
    "html2canvas": "^1.0.0-rc.5",
    "intro.js": "2.9.3",
    "iv-viewer": "^2.0.1",
    "moment": "2.24.0",
    "ng-intercom": "8.0.0",
    "ng2-currency-mask": "^5.3.1",
    "ng2-file-upload": "1.3.0",
    "ng5-slider": "1.1.14",
    "ngx-autosize": "1.6.4",
    "ngx-bootstrap": "3.2.0",
    "ngx-image-drawing": "^1.2.3",
    "ngx-mask": "^8.1.6",
    "ngx-select-dropdown": "1.0.1",
    "ngx-toastr": "9.1.1",
    "npn-slider": "^1.3.0",
    "rxjs": "~6.4.0",
    "rxjs-compat": "^6.4.0",
    "sweetalert2": "8.0.5",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.3",
    "@angular/language-service": "~7.2.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.21",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^4.7.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tsickle": "0.34.2",
    "tslib": "^1.9.0",
    "tslint": "~5.12.1",
    "typescript": "3.2.4"
  }
}
